{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "module": "esnext",
    "target": "es2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {},
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}

